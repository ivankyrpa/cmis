<%- model_class = AnalysisType -%>
<div class="page-header">
  <h1>Тип анализов</h1>
</div>

<dl class="dl-horizontal">
  <dt><strong>Название:</strong></dt>
  <dd><%= @analysis_type.name %></dd>
</dl>

<div class="form-actions">
  <%= link_to "Отмена",
              analysis_types_path, :class => 'btn'  %>
  <%= link_to "Изменить",
              edit_analysis_type_path(@analysis_type), :class => 'btn' %>
  <%= link_to "Удалить",
              analysis_type_path(@analysis_type),
              :method => 'delete',
              :data => { :confirm => "Вы уверены?",
                                  :title => "Подтверждение удаления"  },
              :class => 'btn btn-danger awhite' %>
</div>
