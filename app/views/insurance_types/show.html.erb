<%- model_class = InsuranceType -%>
<div class="page-header">
  <h1>Тип страхования</h1>
</div>

<dl class="dl-horizontal">
  <dt><strong>Тип:</strong></dt>
  <dd><%= @insurance_type.name %></dd>
</dl>

<div class="form-actions">
  <%= link_to "Отмена",
              insurance_types_path, :class => 'btn'  %>
  <%= link_to "Изменить",
              edit_insurance_type_path(@insurance_type), :class => 'btn' %>
  <%= link_to "Удалить",
              insurance_type_path(@insurance_type),
              :method => 'delete',
              :data => { :confirm => "Вы уверены?",
                                  :title => "Подтверждение удаления"  },
              :class => 'btn btn-danger awhite' %>
</div>
