<%- model_class = AgeGroup -%>
<div class="page-header">
  <h1>Возрастная группа</h1>
</div>

<dl class="dl-horizontal">
  <dt><strong>Диапазон:</strong></dt>
  <dd><%= @age_group.from.to_s + "..." + @age_group.to.to_s%></dd>
</dl>

<div class="form-actions">
  <%= link_to "Отмена",
              age_groups_path, :class => 'btn'  %>
  <%= link_to "Изменить",
              edit_age_group_path(@age_group), :class => 'btn' %>
  <%= link_to "Удалить",
              age_group_path(@age_group),
              :method => 'delete',
              :data => { :confirm => "Вы уверены?",
                                  :title => "Подтверждение удаления"  },
              :class => 'btn btn-danger awhite' %>
</div>
